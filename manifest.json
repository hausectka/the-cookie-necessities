{
    "manifest_version": 3,
    "name": "the-cookie-necessities",
    "version": "1.0",
    "description": "Automatically block non-essential analytics/performance/marketing cookies, allow only necessary ones.",
    "permissions": [
        "contentSettings", "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "backgorund": {
        "service_worker": "filter.js"
    },

    "action":{
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "necessary cookies only"
    }
}

